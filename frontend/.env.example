# Spec-Logic Frontend Environment Variables

# Algolia Configuration (Public keys - safe for client-side)
NEXT_PUBLIC_ALGOLIA_APP_ID=your_app_id_here
NEXT_PUBLIC_ALGOLIA_SEARCH_KEY=your_search_key_here
NEXT_PUBLIC_ALGOLIA_INDEX_NAME=prod_components

# Agent Studio Configuration
NEXT_PUBLIC_AGENT_ID=your_agent_id_here

# Admin key for server-side operations (never expose to client)
ALGOLIA_ADMIN_KEY=your_admin_key_here

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
